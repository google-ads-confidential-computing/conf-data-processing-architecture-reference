bazel-*
!bazel-buildifier
.project
/.ijwb/
/.aswb/
/.clwb/
.vscode
.idea/
*.iml
*~
tf_dev_test_env/
.terraform

# NOTE: .terraform.lock.hcl for environments should be checked into git
# https://www.terraform.io/docs/language/dependency-lock.html#lock-file-location
# First, ignore all coordinator .terraform.lock.hcl files by default
/coordinator/terraform/**/.terraform.lock.hcl
# Then Un-ignore .terraform.lock.hcl for mp_primary and mp_secondary environments
!/coordinator/terraform/*/environments_mp_primary/**/*.terraform.lock.hcl
!/coordinator/terraform/*/environments_mp_secondary/**/*.terraform.lock.hcl

# Ignore generated files for eclipse lsp
/pom.xml
/.settings
/.classpath
/.factorypath
/target
/bin
/tools/opensource/license_headers/addlicenses_output.txt
.eclipse

# Ignore node modules
/node_modules
typescript/node_modules


#Ignore generated cache from depedency and licenses check
/tools/opensource/__pycache__/
/tools/opensource/deps.txt

#Ignore release notes file
release_diff_*.txt

MODULE.bazel.lock
